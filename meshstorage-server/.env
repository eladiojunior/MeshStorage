# --> H2 Memória (para Desenvolvimento)
# -> Para utilizar, lembre-se de ajustar o POM.xml
DB_URL_JDBC=jdbc:h2:mem:meshstorage-db
DB_USERNAME=sa
DB_PASSWORD=
DB_DRIVER_CLASS_NAME=org.h2.Driver
DB_JPA_DIALECT=org.hibernate.dialect.H2Dialect
DB_JPA_SHOW_SQL=true

# Configuração do Upload e Requests
LIMIT_MAX_UPLOADFILE_MB=20

# Configuração do Link de acesso ao arquivo (%s = parametro TOKEN)
ACESSO_ARQUIVO_URL=http://192.168.1.24:3001/api/v1/file/link/%s

# Configuração da quanlidade (0 - 1) de compressão em WebP
QUALITY_COMPRESSED_WEBP=0.65f

# Configuração Enfilerador para processar OCR
FILA_PROCESSAMENTO_OCR_URL=
FILA_PROCESSAMENTO_OCR_TOPIC=EXTRACT_OCR_FILE

# Configuração dos PESOS para cálculo do Score para o melhor Storage
# --> Total dos valores deve dar: 1 = 100%
WEIGHT_FREE_SPACE         =0.40; # 40% => Espaço disponível no Storage.
WEIGHT_RESPONSE_TIME      =0.25; # 25% => Tempo de resposta do upload no Storage (quanto menor, melhor).
WEIGHT_REQUEST_LAST_MINUTE=0.25; # 25% => Número uploads no último minuto para o Storage.
WEIGHT_ERRORS_LAST_REQUEST=0.10; # 10% => Número de erros nas últimas 10 requisições para Storage (quanto menor, melhor).