# Configuração do Serviço
SERVER_PORT=3001

# Configuração de Banco de dados
# --> MYSQL
#DB_URL_JDBC=jdbc:mysql://192.168.1.21:3306/db_meshstorage?serverTimezone=UTC&useSSL=false
